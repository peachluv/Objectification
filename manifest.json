{
    "manifest_version": 2,
  
    "name": "Objection Objector",
    "description": "OBJECTION!",
    "version": "0.01",
  
    "icons": {
      "128": "phoenix_icon2.png"
    },
  
    "permissions": [
      "tabs", "<all_urls>"
    ],

    "background": {
      "scripts": ["popup.js"],
      "persistent": true
  },
    "web_accessible_resources": [ "*.mp3", "phoenix-objection.mp3","phoenix_icon2.png"],

    "browser_action": {
      "default_icon": "phoenix_icon2.png",
      "default_popup": "popup.html"
    }
  }